<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="product_template_branding_image_view" model="ir.ui.view">
        <field name="name">product.template.branding.form.view.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='166']" position="after">
                <h4>Preview</h4>
                <br/>
                <field name="branding_image" widget="image" class="oe_avatar" help="Branding Image" nolabel="1"/>
                <br/>


            </xpath>
        </field>
    </record>


</odoo>