<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record id="project_mockup_stage_new" model="project.task.type">
        <field name="sequence">1</field>
        <field name="name">New</field>
        <field name="is_mockup" eval="True"/>
    </record>
    <record id="project_mockup_stage_assigned" model="project.task.type">
        <field name="sequence">10</field>
        <field name="name">Assigned</field>
        <field name="is_mockup" eval="True"/>
    </record>
    <record id="project_mockup_stage_in_progress" model="project.task.type">
        <field name="sequence">10</field>
        <field name="name">In Progress</field>
        <field name="is_mockup" eval="True"/>
    </record>
    <record id="project_mockup_stage_waiting" model="project.task.type">
        <field name="sequence">10</field>
        <field name="name">Waiting Customer Approval</field>
        <field name="is_mockup" eval="True"/>
    </record>
    <record id="project_mockup_stage_approved" model="project.task.type">
        <field name="sequence">10</field>
        <field name="name">Approved</field>
        <field name="is_mockup" eval="True"/>
    </record>
    <record id="project_mockup_stage_rejected" model="project.task.type">
        <field name="sequence">20</field>
        <field name="name">Rejected</field>
        <field name="fold" eval="True"/>
        <field name="is_mockup" eval="True"/>
    </record>
    <record id="project_mockup_stage_cancel" model="project.task.type">
        <field name="sequence">30</field>
        <field name="name">Canceled</field>
        <field name="fold" eval="True"/>
        <field name="is_mockup" eval="True"/>
    </record>

</odoo>
